/* ScrollBar vertical */
QScrollBar:vertical {
        /* border: 0px solid #6f80aa; */
        background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop: 0 #222, stop: 1 #222);
        width: 15px;
        margin: 17px 0 17px 0;
}
QScrollBar::handle:vertical {
        border: none;
        background: qlineargradient(x1: 1, y1: 0, x2: 0, y2: 0, stop: 0 #000, stop: 0.15 #111, stop: 0.5 #770000, stop: 0.85 #111, stop: 1.0 #000);
        min-height: 10px;
}
QScrollBar::handle:vertical:hover {
        border: none;
        background: qlineargradient(x1: 1, y1: 0, x2: 0, y2: 0, stop: 0 #000, stop: 0.05 #111, stop: 0.5 #770000, stop: 0.95 #111, stop: 1.0 #000);
        min-height: 10px;
}
QScrollBar::add-line:vertical  {
        /* border: 0px solid #6f80aa; */
        background: #333;
        height: 15px;
        subcontrol-position: bottom;
        subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical {
        /* border: 0px solid #6f80aa; */
        background: #333;
        height: 15px;
        subcontrol-position: top;
        subcontrol-origin: margin;
}
