/* TabBar */
QTabWidget#permTabWidget::pane {
        border: 1px solid #770000;
}
QTabBar {
	background-color: #222;
	margin-bottom: 4px;
	margin-top: 4px;
	padding-top: 3px;
	padding-left: 3px;
}
QTabBar::tab {
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #770000, stop: 0.33 #0d0d0d, stop: 0.5 #0d0d0d, stop: 0.66 #0d0d0d, stop: 1.0 #770000);
	border: 1px solid #770000;
	border-radius: 3px;
	color: #fff;
	min-width: 8ex;
	padding: 3px;
	padding-top: 3px;
	padding-left: 6px;
	padding-bottom: 3px;
	padding-right: 6px;
}
QTabBar::tab:selected {
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #770000, stop: 0.33 #000, stop: 0.5 #000, stop: 0.66 #000, stop: 1.0 #770000);
	color: #fff;
}
