	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #770000, stop: 0.33 #220000, stop: 0.5 #000, stop: 0.66 #220000, stop: 1.0 #770000);
	color: #fff;
}
QTabBar::tab:selected {
	border-color: #fff;
	border: 1px solid #da0000;
	border-radius: 3px;
}

